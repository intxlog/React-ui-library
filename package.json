{
  "name": "@intxlog/iuilib",
  "version": "0.4.1",
  "description": "A UI library for use in React Apps.",
  "main": "dist/lib/index.js",
  "module": "dist/lib/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "start": "start-storybook -p 6006",
    "start-linked": "npm link && webpack --config webpack.dev.config.js",
    "build": "webpack --config webpack.prod.config.js",
    "build-storybook": "build-storybook -o docs",
    "test": "jest --watch",
    "test-all": "jest --watchAll",
    "test-all-coverage": "jest --watchAll --coverage",
    "test-cover": "jest --coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intxlog/react-ui-library.git"
  },
  "keywords": [
    "javascript",
    "react"
  ],
  "author": "Integrity Express Logistics",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/intxlog/react-ui-library/issues"
  },
  "homepage": "https://intxlog.github.io/react-ui-library",
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/setupTests.js",
      "!src/**/_story.js",
      "!src/lib/index.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "peerDependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@storybook/addon-actions": "^5.0.0",
    "@storybook/addon-info": "^5.0.0",
    "@storybook/addon-links": "^5.0.0",
    "@storybook/addons": "^5.0.0",
    "@storybook/react": "^5.0.0",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.3.0",
    "node-sass": "^4.11.0",
    "opn": "^5.4.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^7.1.0",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  }
}
