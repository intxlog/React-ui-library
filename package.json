{
  "name": "@intxlog/iuilib",
  "version": "0.4.0",
  "description": "A UI library for use in React Apps.",
  "main": "dist/lib/index.js",
  "module": "dist/lib/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "./node_modules/.bin/webpack --mode=production",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest",
    "cover": "npm test -- --coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intxlog/react-ui-library.git"
  },
  "keywords": [
    "javascript",
    "react"
  ],
  "author": "Integrity Express Logistics",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/intxlog/react-ui-library/issues"
  },
  "homepage": "https://intxlog.github.io/react-ui-library",
  "peerDependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/setupTests.js",
      "!src/**/_story.js",
      "!src/lib/index.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@storybook/addon-info": "^5.0.0"
  },
  "devDependencies": {
    "@storybook/react": "^5.0.0",
    "@storybook/addon-actions": "^5.0.0",
    "@storybook/addon-links": "^5.0.0",
    "@storybook/addons": "^5.0.0",
    "@babel/core": "^7.3.4",
    "babel-loader": "^8.0.5"
  }
}
